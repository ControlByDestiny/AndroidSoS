<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToolBar">
        <attr name="title" format="string|reference"/><!--文字内容-->
        <attr name="titleSize" format="dimension|reference"/><!--文字大小-->
        <attr name="titleColor" format="color|reference"/><!--文字颜色-->
        <attr name="rightIcon" format="color|reference"/><!--右边图片-->
        <attr name="leftIcon" format="color|reference"/><!--左边图片-->
        <attr name="backgroundColor" format="color|reference"/><!--背景颜色-->
    </declare-styleable>
</resources>
